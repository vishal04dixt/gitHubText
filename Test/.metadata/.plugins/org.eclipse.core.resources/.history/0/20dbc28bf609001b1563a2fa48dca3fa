import java.io.File;

public class NewFile {{
	
	try {
	File file = new File("D:\\Maven"+File.separator+"check.txt");

	
	if (file.createNewFile()) {
		System.out.println("file created");
		
	}else {
		System.out.println("File not created");
	}
	
			
}catch(Exception e) {
	 e.printStackTrace();
	
}

}
}
