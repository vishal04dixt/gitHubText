import java.io.File;
import java.io.IOException;

public class CreateFile {

	public static void main(String[] args) {
		
		File f = new File("D:\\Maven\\f.txt");
		
		try {
			if(f.createNewFile()) {
				System.out.println("file creted");
			}
		} catch (IOException e) {
			e.printStackTrace();
		}

	}

}
