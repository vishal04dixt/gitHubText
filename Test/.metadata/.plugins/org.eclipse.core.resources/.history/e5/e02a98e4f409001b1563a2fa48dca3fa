import java.io.File;

public class NewFile {{
	
	try {
	File file = new File("D:\\Maven"+File.separator+"check.txt");

	
	if (!file.exists()) {
		file.mkdir();
		File f = new File("D:\\Maven"+File.separator+"check.txt/f.txt");
		f.createNewFile();
	}else {
		System.out.println("File not created");
	}
	
			
}catch(Exception e) {
	 e.printStackTrace();
	
}

}
}
